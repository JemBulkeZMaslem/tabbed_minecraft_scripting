# Compile to test_strings.txt
# ams -i valid_test_commands.ams -o test_strings.txt 
attribute @s minecraft:generic.max_health
  get
  get 1
  get 1.0
  base
    get
    get 3
    get 3.5
    set 15
    set 2

  modifier
    add f81d4fae-7dec-11d0-a765-00a0c91e6bf6 TankyBoi 15 multiply
    value get
      f81d4fae-7dec-11d0-a765-00a0c91e6bf6
      f81d4fae-7dec-11d0-a765-00a0c91e6bf6 1
      f81d4fae-7dec-11d0-a765-00a0c91e6bf6 1.1
    remove f81d4fae-7dec-11d0-a765-00a0c91e6bf 6

    add a-a-a-a Fragile -8 add

bossbar
  add test Test
  get test visible
  list
  set test
    color yellow
    max 20
    name THISISATEST
    players @a[tag=tester, limit=1, sort=nearest]
    style notched_20
    value 20
    visible true
  remove test

clear
  @a
  @p minecraft:stone
  @s minecraft:stone 1

data
  get
    block 0 0 0
    block 0 0 0 Items
    get entity @s Inventory[0].Count 2

  merge block 0 0 0  {Items: []}

  modify
    block 0 0 0
      append from
        block 0 0 0
        block 0 0 0 Items
        get entity @s Inventory[0].Count

      remove Items

    block 0 0 0 Items
      append
        block 0 0 0
        block 0 0 0 Items
        get entity @s Inventory[0].Count

      append value
        {Slot: 4b, id:"minecraft:stone", Count: 1b}

    get entity @s Inventory[0]
      append
        block 0 0 0
        block 0 0 0 Items
        get entity @s Inventory[0].Count

      append value
        3

effect
  clear @a minecraft:blindness
  give @s
    minecraft:blindness
    minecraft:blindness 15
    minecraft:blindness 15 3
    minecraft:blindness 15 1 true

enchant @a
  minecraft:vanishing_curse
  minecraft:efficiency 4

function datapack:namespace/function

gamemode survival @a

give @s
  minecraft:acacia_boat
  minecraft:acacia_boat 3

kill @a

list

say Hello @a, how are you today? It's 15 degrees outside and I am sick.

stop

tag @s
  add tester
  list
  remove tester

team
  add cool_team SLAYER_GURLS
  empty cool_team
  join
    cool_team
    cool_team @a
  list
  list cool_team
  leave @a

  modify cool_team
    color green
    deathMessageVisibility never
    seeFriendlyInvisibles true
    friendlyFire false

  remove cool_team

teleport @a
  @s
  0 0 0
  0 64 0 facing
    0 64 1
    entity
      @p[tag=tester]
      @p[tag=tester] head

tellraw @a
  {"text":"boop"}
  [{"text":"boop"}]

title @a
  clear
  title
    {"text":"boop"}
    [{"text":"boop"}]

  times
    1 1 1
    1.0 1.0 1.0
